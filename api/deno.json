{
  "importMap": "./imports.json",
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any",
        "ban-types",
        "no-local",
        "no-inferrable-types",
        "no-prototype-builtins",
        "no-cond-assign",
        "strict-boolean-expressions",
        "@typescript-eslint/strict-boolean-expressions"
      ]
    }
  },
  "fmt": {
    "options": {
      "singleQuote": true
    }
  },
  "compilerOptions": {
    "noImplicitAny": false
  },
  "lock": false
}
